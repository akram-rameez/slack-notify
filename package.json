{
  "name": "node-ts-starter",
  "version": "2.0.0",
  "main": "index.js",
  "author": "Horus Lugo",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd --respawn --files -r @babel/register --inspect -- src/index.ts",
    "build": "./node_modules/babel-cli/bin/babel ./src -d ./dist --extensions .ts --copy-files",
    "typecheck": "tsc",
    "start": "node dist/index.js",
    "lint:base": "eslint --ext .js,.json,.ts",
    "lint": "yarn lint:base src"
  },
  "lint-staged": {
    "src/**/*.{ts,js,json}": [
      "yarn lint:base --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck && lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@types/death": "^1.1.1",
    "@types/mongodb": "^3.6.12",
    "@types/node": "^14.14.16",
    "@types/node-cron": "^2.0.3",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@slack/web-api": "^6.1.0",
    "add": "^2.0.6",
    "assert": "^2.0.0",
    "babel-cli": "^6.26.0",
    "core-js": "^3.12.1",
    "cowin-api-client": "^0.0.2",
    "death": "^1.1.0",
    "mongodb": "^3.6.6",
    "node-cron": "^3.0.0",
    "redis": "^3.1.2",
    "regenerator-runtime": "^0.13.7",
    "yarn": "^1.22.10"
  }
}
